package onlinelearning;

public class leftRotate {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a[]= {2,6,7,8,3,5};
		int n = a.length;
		int temp =a[0];
		for(int i=0;i<n-1;i++) {
			a[i]=a[i+1];
			
		}
		a[n-1]=temp;
		for(int j :a) {
			System.out.println(j);
		}

	}

}
