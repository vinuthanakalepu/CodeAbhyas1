package onlinelearning;

public class arraySortedOrNot {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a[]= {1,2,3,4,5};
		int n= a.length;
		boolean c= true;
		for(int i=0;i<n-1;i++) {
			if(a[i]<a[i+1]) {
				System.out.println(c);
			}
			else {
				System.out.println("not sorted");
			}
			
		}

	}

}
