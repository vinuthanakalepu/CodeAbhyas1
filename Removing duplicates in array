package onlinelearning;

public class removingduplicates {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a [] = {1,1,1,2,3,4,4

	};
		int n =a.length,count=1;
		int i=0;
		for(int j=1;j<n;j++) {
			if(a[i]!=a[j]) {
				a[i+1]=a[j];
				i++;
				a[i]=a[j];
				count++;
			}
		}
		for(int j=0;j<count;j++) {
			System.out.println(a[j]);
		}

}
}
